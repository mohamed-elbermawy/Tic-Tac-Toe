<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="770.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.SignUpController">
   <children>
      <Label layoutX="314.0" layoutY="71.0" prefHeight="50.0" prefWidth="200.0" text="Register">
         <font>
            <Font name="padmaa-Bold.1.1" size="30.0" />
         </font>
      </Label>
      <TextField fx:id="username" layoutX="234.0" layoutY="160.0" prefHeight="40.0" prefWidth="300.0" promptText="Enter username" />
      <TextField fx:id="password" layoutX="234.0" layoutY="210.0" prefHeight="40.0" prefWidth="300.0" promptText="Enter password" />
      <TextField fx:id="confirmPassword" layoutX="234.0" layoutY="260.0" prefHeight="40.0" prefWidth="300.0" promptText="confirm password" />
      <Button fx:id="registerSignUpBtn" layoutX="234.0" layoutY="319.0" mnemonicParsing="false" onAction="#signUpProccess" prefHeight="40.0" prefWidth="300.0" text="Sign Up">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="registerBackBtn" layoutX="234.0" layoutY="370.0" mnemonicParsing="false" onAction="#back" prefHeight="40.0" prefWidth="300.0" text="Back">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
